          ]
        }
      ],
      "source": [
        "## Multinomial Naive Bayes\n",
        "print(\"Multinamial NB----------------------------------->\")\n",
        "multinb = MultinomialNB()\n",
        "multinb.fit(X_train_2, y_train)\n",
        "\n",
        "y_train_pred = multinb.predict(X_train_2)\n",
        "y_test_pred = multinb.predict(X_test_transform)\n",
        "classifier_scores(y_train,y_test,y_train_pred,y_test_pred)"
      ]
    },
